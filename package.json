  "scripts": {
    "build": "tailwindcss  -i ./src/index.css -o ./dist/css/calendar.css",
    "dev": "tailwindcss  -i ./src/index.css -o ./dist/css/calendar.css --watch"
  },
  "repository": "https://github.com/nintrealab/tailwindcss-calendar.git",
