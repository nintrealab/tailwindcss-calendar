  "author": "Leat Sophat",
  "license": "MIT",
  "homepage": "https://nintrealab.github.io/tailwindcss-calendar/",
  "devDependencies": {
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "tailwindcss-calendar": "^1.0.3"
  },
